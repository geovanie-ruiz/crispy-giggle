{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "lint-and-build",
			"type": "shell",
			"command": "npm run lint && npm run build",
			"isBackground": false,
			"problemMatcher": [
				"$eslint-stylish"
			],
			"group": "build"
		},
		{
			"label": "lint-current-file",
			"type": "shell",
			"command": "eslint src/app/page.tsx",
			"isBackground": false,
			"problemMatcher": [
				"$eslint-stylish"
			],
			"group": "build"
		}
	]
}